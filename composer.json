{
    "name": "LSS/threema-gateway",
    "type": "library",
    "description": "Threema PHP-SDK to integrate Threema Gateway in your environment. This SDK allows you to send end-to-end-encrypted messages to Threema users using the Threema Gateway.",
    "keywords": [
        "threema",
        "gateway"
    ],
    "homepage": "https://gateway.threema.ch/en/developer/sdk-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Threema GmbH",
            "email": "support-gateway-service@threema.ch",
            "homepage": "https://gateway.threema.ch",
            "role": "Original Developer"
        },
        {
            "name": "rugk",
            "email": "rugk@posteo.de",
            "homepage": "https://github.com/rugk",
            "role": "Semi-official fork maintainer"
        },
        {
            "name": "pavarnos",
            "email": "pavarnos@gmail.com",
            "homepage": "https://github.com/pavarnos",
            "role": "Maintain this fork"
        }
    ],
    "autoload": {
        "psr-4": {
            "Threema\\": "source/Threema"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Threema\\": "tests/Threema"
        }
    },
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-sodium": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "pds/skeleton": "^1.0"
    },
    "bin": [
        "src/threema"
    ],
        "scripts": {
        "test": "./vendor/bin/phpunit"
    }
}
